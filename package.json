{
  "name": "webpack-blog-code",
  "version": "1.0.0",
  "description": "Code example for blog post",
  "main": "dist/app.js",
  "scripts": {
    "postinstall": "rimraf typings & typings install",
    "clean": "rimraf dist",
    "bundle": "rollup -c",
    "6to5": "babel dist/app-es6.js --out-file dist/app.js",
    "minify": "uglifyjs dist/app.js --screw-ie8 --compress --mangle --output dist/app.min.js",
    "build": "npm run clean && npm run bundle && npm run 6to5 && npm run minify",
    "serve": "npm run build && lite-server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {},
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/node": "6.0.45",
    "babel-cli": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "lite-server": "2.2.2",
    "rimraf": "2.5.4",
    "rollup": "0.36.3",
    "rollup-plugin-alias": "1.2.0",
    "rollup-plugin-commonjs": "5.0.5",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-typescript": "0.8.1",
    "rollup-plugin-uglify": "1.0.1",
    "typescript": "2.0.3",
    "uglify-js": "2.7.3"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.12",
    "rxjs-es": "5.0.0-beta.12",
    "zone.js": "0.6.21"
  }
}
